{
  "name": "xers",
  "description": "Generate illustration stats cards for your GitHub readme",
  "version": "1.0.50",
  "keywords": [
    "github-readme-stats",
    "readme-stats",
    "cards-art",
    "github-card-art"
  ],
  "main": "build/index.js",
  "scripts": {
    "build:vercel": "bun run build && bun scripts/transform-paths.ts",
    "build": "bun build ./src/index.ts --outdir ./build",
    "start": "bun build/index.js",
    "test": "bun test",
    "dev": "bun run --watch src/index.ts"
  },
  "dependencies": {
    "@elysiajs/swagger": "^0.7.4",
    "dotenv": "^16.3.1",
    "elysia": "latest"
  },
  "devDependencies": {
    "bun-types": "latest"
  },
  "module": "build/index.js"
}